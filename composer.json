{
    "name": "featurit/featurit-sdk-php",
    "description": "PHP SDK for FeaturIT",
    "keywords": ["featurit", "feature flag", "feature toggle", "user targeting", "a b testing", "feature", "feature management"],
    "license": "MIT",
    "authors": [
        {
            "name": "FeaturIT",
            "email": "featurit.tech@gmail.com",
            "homepage": "https://www.featurit.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Featurit\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Featurit\\Client\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "laminas/laminas-serializer": "^2.14",
        "lastguest/murmurhash": "^2.1",
        "php-http/client-common": "^2.3",
        "php-http/discovery": "^1.12",
        "psr/http-client": "^1.0",
        "psr/http-client-implementation": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-factory-implementation": "^1.0",
        "psr/simple-cache": "^1.0 || ^2.0 || ^3.0",
        "psr/simple-cache-implementation": "^1.0 || ^2.0 || ^3.0",
        "symfony/cache": "^5.0 | ^6.0 | ^7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "laminas/laminas-diactoros": "^2.4",
        "php-http/curl-client": "^2.1",
        "php-http/mock-client": "^1.5",
        "phpunit/phpunit": "^9",
        "symfony/http-client": "^5.0 | ^6.0 | ^7.0",
        "symfony/var-dumper": "^5.1"
    }
}
